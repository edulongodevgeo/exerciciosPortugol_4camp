//Leia as medidas(b, h) de um retângulo. 
//Escreva a área calculada.

programa {
	
	
	funcao inicio() {
	    
	    inteiro b, h, area
	    
	    escreva("Digite o valor do lado B do retângulo: \n")
	    leia(b)
	    
	    escreva("Digite o valor do lado h do retângulo: \n")
	    leia(h)
	    
	    area = b*h
	    
	    escreva("A área do retângulo será igual a ", area)
		
	}
}
